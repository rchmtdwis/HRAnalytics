					"SqlServerKelompok1_properties_typeProperties_server": {
						"value": "[parameters('SqlServerKelompok1_properties_typeProperties_server')]"
					},
					"SqlServerKelompok1_properties_typeProperties_database": {
						"value": "[parameters('SqlServerKelompok1_properties_typeProperties_database')]"
					},
					"SqlServerKelompok1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerKelompok1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
